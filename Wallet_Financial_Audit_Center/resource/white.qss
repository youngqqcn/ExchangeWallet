/**********按钮**********/
QPushButton{
    border-radius: 3px;
    font:14px "微软雅黑";
    border: none;
    outline:none;
}
QPushButton:enabled {
    background: rgb(120, 170, 220);
    font:14px "微软雅黑";
    color: white;
}
QPushButton:!enabled {
    font:14px "微软雅黑";
    background: rgb(180, 180, 180);
    color: white;
}
QPushButton:enabled:hover{
    font:14px "微软雅黑";
    background: rgb(100, 160, 220);
}
QPushButton:enabled:pressed{
    font:14px "微软雅黑";
    background: rgba(0, 78, 161);
