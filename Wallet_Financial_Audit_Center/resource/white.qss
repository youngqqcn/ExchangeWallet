QComboBox QAbstractItemView {
    border : 1px solid rgb(111, 156, 207);
    background: white;
    font-size: 13px;
    outline: none;
}
QComboBox QAbstractItemView::item {
    color: rgb(73, 73, 73);
    height: 25px;
}
QComboBox QAbstractItemView::item:selected {
    background: rgb(232, 241, 250);
    color: rgb(2, 65, 132);
}

QComboBox#cbx_skin{
    border:none;
    font: 13px "微软雅黑";
    background: rgb(120, 170, 220);
    width: 80px;
    height: 30px;
}
QComboBox#cbx_skin:hover{
    background: rgb(130, 180, 230);
}
QComboBox#cbx_skin::drop-down {
